{
  "languageTag": "en-US",
  "fileDescription": "Strings for GameDistribution SDK.",
  "text": {
    "plugins": {
      "gd_sdk": {
        "name": "GameDistribution SDK",
        "description": "Construct 3 plugin for the GameDistribution SDK",
        "help-url": "https://www.gamedistribution.com/sdk",
        "properties": {
          "game-id": {
            "name": "Game ID",
            "desc": "Enter your Game ID (required)"
          },
          "level-text": {
            "name": "Level",
            "desc": "Enter level object"
          },
          "score-text": {
            "name": "Score",
            "desc": "Enter score object"
          }
        },
        "aceCategories": {
          "custom": "Custom"
        },
        "conditions": {
          "pause-game": {
            "list-name": "Pause game during advertisement",
            "display-text": "Advertisement is active.",
            "description": "Pause game during advertisement"
          },
          "resume-game": {
            "list-name": "Resume game after advertisement",
            "display-text": "Advertisement is not active.",
            "description": "Resume game after advertisement"
          },
          "reward-player": {
            "list-name": "Give reward here",
            "display-text": "Rewarded Ad succeed.",
            "description": "Rewarded Ad watched completely."
          },
          "preloaded-ad": {
            "list-name": "Is rewarded advertisement available.",
            "display-text": "Preloaded advertisement",
            "description": "Enable/Show rewarded button if available."
          }
        },
        "actions": {
          "show-ad": {
            "list-name": "Show an advertisement.",
            "display-text": "Show an advertisement",
            "description": "Tries to display an advertisement. Make sure to also handle the appropriate conditions."
          },
          "show-rewarded-ad": {
            "list-name": "Show a rewarded advertisement.",
            "display-text": "Show a rewarded advertisement",
            "description": "Tries to display a rewarded advertisement. Make sure to also handle the appropriate conditions."
          },
          "preload-rewarded-ad": {
            "list-name": "Preload a rewarded advertisement",
            "display-text": "Preload a rewarded advertisement",
            "description": "Tries to preload a rewarded advertisement. Make sure to also handle the appropriate conditions."
          },
          "send-game-event": {
            "list-name": "Send game event",
            "display-text": "Send game event",
            "description": "Tries to send score and level information to top frame."
          }
        }
      }
    }
  }
}
